    "babel-loader": "^8.2.2",
    "cross-env": "^7.0.3",
    "electron": "^13.2.1",
    "file-loader": "^6.2.0",
    "terser-webpack-plugin": "^5.1.4",
    "vue-loader": "^15.9.8",
    "vue-template-compiler": "^2.6.14",
    "webpack": "^5.51.1",
    "webpack-cli": "^4.8.0"
  },
  "dependencies": {
    "better-sqlite3": "^7.4.3",
    "electron-squirrel-startup": "^1.0.0",
